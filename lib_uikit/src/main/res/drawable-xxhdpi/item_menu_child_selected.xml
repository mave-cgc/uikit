<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <solid android:color="#EF7143" />
            <stroke android:width="1px" android:color="#EF7143" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#F3F5F8" />
            <stroke android:width="1dp" android:color="#E7E9EB" />
            <corners android:radius="5dp" />
        </shape>
    </item>
</selector>